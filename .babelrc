{
	"presets": [
		"@babel/preset-env"
	],
	"plugins": [
		[
			"@babel/plugin-transform-runtime",
			{
				"corejs": false,
				"helpers": true,
				"regenerator": true,
				"useESModules": false
			}
		]
	],
	"env": {
		"test": {
			"plugins": [
				"transform-es2015-modules-commonjs"
			]
		}
	}
}